<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>

        <shape android:shape="rectangle">
            <corners android:radius="5dp" />

            <solid android:color="#5F9EA0" />

            <stroke
                android:width="1dp"
                android:color="#FFFFFF" />

            <padding
                android:bottom="1dp"
                android:top="1dp"
                android:left="10dp"
                android:right="10dp" />

        </shape>
    </item>


</layer-list>